{
  "name": "todos",
  "version": "0.0.1",
  "engines": {
    "node": ">=18"
  },
  "private": true,
  "workspaces": [
    "docs",
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "dev": "turbo run dev",
    "prep": "npm install && npm run format && npm run build",
    "codegen": "turbo run codegen"
  },
  "devDependencies": {
    "turbo": "1.13.2"
  },
  "packageManager": "npm@8.7.0"
}
