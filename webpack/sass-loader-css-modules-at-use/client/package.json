{
  "name": "client",
  "version": "0.0.1",
  "description": "frontend for gg-test",
  "private": true,
  "keywords": [
    "frontend, webpack, js, css, sass"
  ],
  "author": "gabengar",
  "license": "MIT",
  "imports": {
    "#configs/*": "./configs/*",
    "#api": "./src/api/_index.js",
    "#components": "./src/components/_index.js",
    "#components/fancy": "./src/components/fancy/_index.js",
    "#components/forms": "./src/components/forms/_index.js",
    "#pages": "./src/pages/_index.js",
    "#assets": "./src/assets/_index.js",
    "#assets/*": "./src/assets/*",
    "#scripts/*": "./src/scripts/*",
    "#styles/*": "./src/styles/*",
    "#views/*": "./src/views/*"
  },
  "scripts": {
    "predev": "rimraf dev/*",
    "dev": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "date-fns": "^2.20.1",
    "nanoid": "^3.1.25",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.1",
    "dotenv": "^8.2.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.1",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.1.1",
    "webpack-merge": "^5.7.3"
  }
}
